generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Entry {
  id        String   @id @default(uuid())
  content   String
  createdAt DateTime @default(now())
  skills    Skills
  password  String
  email     String   @unique
  name      String
  title     String
}

enum Skills {
  Java
  Python
  C
}
