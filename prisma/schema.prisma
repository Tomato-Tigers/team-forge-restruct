generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model Entry{
  id String  @id @default(uuid())
  password String 
  username String
  content String
  createdAt DateTime @default(now())
  skills Skills
}

enum Skills{
  Java
  Python
  C
  


}